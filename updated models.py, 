        "print(f\"Performing TruncatedSVD with {num_dimensions} components...\")\n",
        "tfidf_svd = svd.fit_transform(sparse_matrix)\n",
        "\n",
        "X_aug_combined = hstack([tfidf_svd, extra_sparse])\n",
